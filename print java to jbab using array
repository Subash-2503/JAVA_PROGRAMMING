import java.util.*;
public class Main
{
	public static void main(String[] args) {
    Scanner sc=new Scanner (System.in);
    String s=sc.nextLine();
    int len=s.length();
    for (int i=0;i<len;i++){
        char g=s.charAt(i);
        if (g=='a'){
            g='b';
        }
        System.out.print(g);
    }
	}
}
